		<field class="long" name="teamid" explain="队伍Id"></field>
		<field class="long" name="playerid" explain="玩家ID"></field>
		<field class="byte" name="type" explain="0更新外观 ，1 更新到GAME ，2更新到前端"></field>
	</message>
	
